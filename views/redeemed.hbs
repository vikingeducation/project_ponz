
<!-- Title -->
  <div class="row">
    <div class="col-lg-12">
      <h3 class="text-center">Your Stuff</h3>
    </div>
  </div>
  <!-- /.row -->
  <br>
  <!-- Page Features -->
  <div class="row text-center">
  <h4>You spent ${{user.pointsSpent}}.00!</h4>
  <hr>
    {{#each user.itemsBought as |item|}}
    <div class="col-md-3 col-sm-6 hero-feature">
      <div class="thumbnail">
        <img src="{{item.image}}" alt="">
        <div class="caption">
          <h3>{{item.name}}</h3>
          <h5>${{item.price}}.00</h5>

        </div>
      </div>
    </div>
    {{/each}}
  </div>
  <!-- /.row -->

  <hr>